    "@types/joi": "^14.3.4",
    "@types/mongoose": "^5.7.36",
    "express": "^4.17.1",
    "joi": "^17.2.0",
    "mongoose": "^5.10.0",
    "reflect-metadata": "^0.1.13"
  }
